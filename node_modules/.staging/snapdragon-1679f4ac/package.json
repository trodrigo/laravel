keywords": [
    "lexer",
    "snapdragon"
  ],
  "verb": {
    "toc": false,
    "layout": "default",
    "tasks": [
      "readme"
    ],
    "plugins": [
      "gulp-format-md"
    ],
    "related": {
      "description": "These libraries use snapdragon:",
      "list": [
        "braces",
        "expand-brackets",
        "extglob",
        "micromatch"
      ]
    },
    "reflinks": [
      "css",
      "pug",
      "verb",
      "verb-generate-readme"
    ],
    "lint": {
      "reflinks": true
    }
  }
}
